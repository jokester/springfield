{
  "name": "springfield",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": false,
  "license": "MIT",
  "description": "React-managed shared element transition",
  "keywords": [
    "react",
    "animation",
    "shared element transition",
    "transition"
  ],
  "bugs": {
    "url": "https://github.com/jokester/springfield/issues"
  },
  "homepage": "https://github.com/jokester/springfield#readme",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "CI=true react-scripts test --env=jsdom --coverage --watchAll=false --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --coverageReporters=html",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "peerDependicies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@types/jest": "^24",
    "@types/node": "<11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "source-map-explorer": "^2.1.0",
    "ts-jest": "^24",
    "typescript": "^3.6.4"
  }
}
